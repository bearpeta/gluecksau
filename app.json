{
  "expo": {
    "name": "Gluecksau",
    "slug": "gluecksau",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "splash": {
      "image": "./assets/app_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#faeff1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bearpeta.gluecksau"
    },
    "android": {
      "package": "com.bearpeta.gluecksau",
      "permissions": ["WAKE_LOCK"]
    }
    
  },
  "packagerOpts": {
    "config": "rn-cli.config.js"
  }
}